{
  "name": "catnem",
  "displayName": "catNem",
  "description": "catnem extention",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": [
      {
        "title": "catnem",
        "properties": {
          "greetings": {
            "type": "string",
            "description": "your greeting",
            "default": "bonjour"
          }
        }
      }
    ],
    "commands": [
      {
        "command": "catnem.helloWorld",
        "title": "helloWorld"
      },
      {
        "command": "catnem.showcat",
        "title": "Show Cat"
      },
      {
        "command": "catnem.countDiagnostics",
        "title": "Count Errors and Warnings"
      },
      {
        "command": "catnem.nem",
        "title": "view NEM"
      }

    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "axios": "^1.7.7"
  }
}
